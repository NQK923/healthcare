<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="1">
    <record id="ir_cron_appointment_reminder" model="ir.cron">
      <field name="name">Gửi nhắc nhở lịch hẹn</field>
      <field name="model_id" ref="healthcare_management.model_healthcare_appointment"/>
      <field name="state">code</field>
      <field name="code">model.send_reminder_emails()</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="active" eval="True"/>
    </record>
  </data>
</odoo>